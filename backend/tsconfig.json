{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "types": ["node"]
  },
  "include": [
    "*.ts",
   "enhanced-k33p-manager.ts",
    "../smart_contract_validator/k33p-deploy/k33p-signup-interactions.ts",
    "enhanced-k33p-manager.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}